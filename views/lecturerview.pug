doctype html
head
  meta(charset='UTF-8')
  title Result Upload
  link(rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/normalize/5.0.0/normalize.min.css')
  link(rel='icon' type='image/x-icon' href='idea.png')
  link(rel='stylesheet' href='./style.css')
  // axios cdn script
  script(src='https://cdnjs.cloudflare.com/ajax/libs/axios/1.4.0/axios.min.js' integrity='sha512-uMtXmF28A2Ab/JJO2t/vYhlaa/3ahUOgj1Zf27M5rOo8/+fcTUVH0/E0ll68njmjrLqOBjXM3V9NiPFL5ywWPQ==' crossorigin='anonymous' referrerpolicy='no-referrer')
  style.
// partial:index.partial.html
.app-container
  .sidebar
    .sidebar-header
      .app-icon
        img(src='idea.png' alt='logo' width='30')
        // 
    ul.sidebar-list
      li.sidebar-list-item.result-mobile.active
        a(href='#')
          svg.feather.feather-home(xmlns='http://www.w3.org/2000/svg' width='18' height='18' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
            path(d='M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z')
            polyline(points='9 22 9 12 15 12 15 22')
          span Results
      li.sidebar-list-item.profile-mobile
        a(href='#')
          svg.feather.feather-shopping-bag(xmlns='http://www.w3.org/2000/svg' width='18' height='18' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
            path(d='M6 2L3 6v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2V6l-3-4z')
            line(x1='3' y1='6' x2='21' y2='6')
            path(d='M16 10a4 4 0 0 1-8 0')
          span Profile
    .account-info
      .account-info-picture
        img(src='idea.png' alt='Account')
      .account-info-name c 2023
      button.account-info-more
        svg.feather.feather-more-horizontal(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
          circle(cx='12' cy='12' r='1')
          circle(cx='19' cy='12' r='1')
          circle(cx='5' cy='12' r='1')
  .app-content
    .app-content-header
      h3.app-content-headerText(style='text-align: center')
        | Electrical Department
      button.mode-switch(title='Switch Theme')
        svg.moon(fill='none' stroke='currentColor' stroke-linecap='round' stroke-linejoin='round' stroke-width='2' width='24' height='24' viewbox='0 0 24 24')
          defs
          path(d='M21 12.79A9 9 0 1111.21 3 7 7 0 0021 12.79z')
    a(href='/logout')
      button.app-content-headerButton Logout
    .app-content-actions
      input.search-bar(value=`Course: ${lecturer.courseTitle}` type='text' readonly='')
      .app-content-actions-wrapper
        // add class here to show menu bar when the screenn is mobile or tablet size change display to block below
        .filter-button-wrapper
          button.action-button.filter.jsFilter
            svg.feather.feather-grid(xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
              rect(x='3' y='3' width='7' height='7')
              rect(x='14' y='3' width='7' height='7')
              rect(x='14' y='14' width='7' height='7')
              rect(x='3' y='14' width='7' height='7')
          .filter-menu
            ul.sidebar-list
              li.sidebar-list-item.result-desktop
                a(href='#')
                  svg.feather.feather-home(xmlns='http://www.w3.org/2000/svg' width='18' height='18' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                    path(d='M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z')
                    polyline(points='9 22 9 12 15 12 15 22')
                  span Results
              li.sidebar-list-item.profile-desktop.active
                a(href='#')
                  svg.feather.feather-shopping-bag(xmlns='http://www.w3.org/2000/svg' width='18' height='18' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                    path(d='M6 2L3 6v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2V6l-3-4z')
                    line(x1='3' y1='6' x2='21' y2='6')
                    path(d='M16 10a4 4 0 0 1-8 0')
                  span Profile
    // result form
    .form-container
      form#formpage
        .form-field
          label.form-label(for='name') RegNo:
          input#regno.form-input(type='text' value=' ' required='')
        .form-field
          label.form-label(for='email') Score:
          input#score.form-input(type='number' max='100' min='0' required='')
        .form-field
          label.dropdown-label(for='country') Level:
          select#level.dropdown-select
            option(value=lecturer.level )= lecturer.level
            option(value='200L') 200L
            option(value='300L') 300L
            option(value='400L') 400L
            option(value='500L') 500L
        .form-field
          label.form-label(for='phone') CourseCode:
          input#CourseCode.form-input(type='text' value=lecturer.courseCode readonly='')
        .form-field
          label.form-label(for='phone') Semester:
          input#Semester.form-input(type='text' value=lecturer.semester  readonly='')
        .form-field
          label.form-label(for='name') Credit Unit:
          input#creditUnit.form-input(type='text' value=lecturer.criditunit readonly='')
        button#result-upload.form-button(type='submit') Upload
    // end of card
    // profile page
    .profile-container.container
      .profile
        img(src='/arinse-ibori.jpeg' alt='User Profile Image')
        h2=lecturer.name
        p=`Email: ${lecturer.email}`
        p=`Course: ${lecturer.courseTitle}`
        p=`Course Code: ${lecturer.courseCode}`
        p=`Credit Unit:  ${lecturer.criditunit}`
        .form-field
          label.form-label(for='phone')
          input#currentpassword.form-input(type='text' placeholder='Current Password')
        .form-field
          label.form-label(for='name')
          input#newpassword.form-input(type='text' placeholder='New Password')
        a.btn.passwordUpdate(href='#') Password Reset
        .form-field
          label.form-label(for='phone')
          input#regno.form-input(type='text' value=lecturer.courseCode hidden)
    // profile ends here
// partial
script(src='./script.js')
script(src='./resultupload.js')
